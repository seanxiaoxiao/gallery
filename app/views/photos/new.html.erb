<h1>New photo</h1>

<%= form_for @photo do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, :size => 20 %>
  </div>
  <div class="field">
    <%= f.label :desc %>
    <%= f.text_field :desc, :size => 20 %>
  </div>
  <div class="field">
    <%= f.file_field :imagefile %>
  </div>
  <%= f.submit %>
<% end %>

<%= link_to 'Back', photos_path %>
